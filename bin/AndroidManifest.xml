<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.abcbank"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
   
        <uses-permission android:name="android.permission.INTERNET" />
		<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

   

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
         >
        <activity
            android:screenOrientation="portrait"
            android:name="com.example.abcbank.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.abcbank.Balances"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_balances" >
        </activity>
        <activity
            android:name="com.example.abcbank.SavingsHistory"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_savings_history" >
        </activity>
        <activity
            android:name="com.example.abcbank.CheckingHistory"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_checking_history" >
        </activity>
        <activity
            android:name="com.example.abcbank.CreditHistory"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_credit_history" >
        </activity>
        <activity
            android:name="com.example.abcbank.Transfer"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_transfer" >
        </activity>
        <activity
            android:name="com.example.abcbank.PayCard"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_pay_card" >
        </activity>
        

    </application>

</manifest>